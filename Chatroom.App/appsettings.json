{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Self": {
    "Id": "bot.api.client",
    "Secret": "secret",
    "Scope": "chatroom.bot.api.full"
  },
  "IdentityClient": {
    "ClientId": "mvc",
    "ClientSecret": "secret",
    "ResponseType": "code",
    "SignedOutRedirectUri": "https://localhost:5002/",
    "SignedOutCallbackPath": "/signout-callback-oidc"
  },
  "ApiResourceBaseUrls": {
    "AuthServer": "https://localhost:5001",
    "BotApi": "http://localhost:5000"
  },
  "HttpClientPolicies": {
    "RetryCount": 3,
    "RetryDelayInMs": 500,
    "RetryTimeoutInSeconds": 5,
    "BreakDurationInSeconds": 30,
    "MaxAttemptBeforeBreak": 3,
    "HandlerTimeoutInMinutes": 5
  }
}
